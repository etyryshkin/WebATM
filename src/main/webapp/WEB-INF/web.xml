<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>accountsList</servlet-name>
        <servlet-class>com.webatm.servlets.AccountsList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>accountsList</servlet-name>
        <url-pattern>/accountsList</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>accountDetails</servlet-name>
        <servlet-class>com.webatm.servlets.AccountDetails</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>accountDetails</servlet-name>
        <url-pattern>/accountDetails</url-pattern>
    </servlet-mapping>
</web-app>
